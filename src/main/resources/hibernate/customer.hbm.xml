<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dycs.Customer.domain.entity">
	<class name="Customer" table="customer">
        <id name="CustomerID" column="customerID">
            <generator class="native"/>
        </id>
        <property name="Name" column="name" not-null="true"/>
        <property name="Email" column="email" not-null="true"/>
        <property name="Status" column="status" not-null="true"/>
        <property name="StatusExpirationDate" column="statusExpirationDate" not-null="true"/>
         <property name="MoneySpent" column="moneySpent" not-null="true"/>
    </class> 
</hibernate-mapping>